# Maintainer: Mirko Rovere <04mirko@libero.it>

pkgname="baracle"
pkgver="1.0"
pkgrel="1"
pkgdesc="A simple bash wrapper to manage one or more archives from the terminal"
arch=("x86_64")
url="https://github.com/Mirko-r/baracle"
depends=('gcc')
license=("GPL-3.0")
source=("https://github.com/Mirko-r/extrattor/releases/download/1.3/extrattor.sh")
sha512sums=("SKIP")

package(){
    install -Dm0755 "${srcdir}/extrattor.sh" "${pkgdir}/usr/bin/extrattor"
}
